{
    "manifest_version" : 2,
    "name" : "Click Bait Predictor",
    "description" : "This extension will predict the probability of a page being a click-bait.",
    "version" : "1.0",
    "background" :{
        "scripts" : ["popup.js", "jquery-3.3.1.min.js"]
    },
    
    "browser_action" : {
        "default_icon" : "icon.png",
        "default_popup" : "popup.html"
    },
    "permissions" : [
        "activeTab"
    ]
}